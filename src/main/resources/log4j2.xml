<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="LogToConsole" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - (%F:%M:%L) %msg%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="com.imdpeach.*" level="trace" additivity="false">
            <AppenderRef ref="LogToConsole"/>
        </Logger>
        <Root level="trace">
            <AppenderRef ref="LogToConsole"/>
        </Root>
    </Loggers>
</Configuration>